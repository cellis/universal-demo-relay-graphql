{
  "name": "flush-requires-boilerplate-webpack-chunknames",
  "description": "Universal Relay with Found and Found HMR, and Code-Splitting with react-universal-component, webpack-flush-chunks extract-css-chunks-webpack-plugin",
  "version": "1.0.0",
  "main": "server/index.js",
  "author": "Cameron Ellis <cameronellis.ellis@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development babel-node server/index.js",
    "start:prod": "npm run clean && cross-env NODE_ENV=production babel-node server/index.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "rimraf build/client && NODE_ENV=production webpack --progress -p --config internal/webpack/client.prod.babel.js",
    "build:server": "rimraf build/server && NODE_ENV=production webpack --progress -p --config internal/webpack/server.prod.babel.js",
    "relay-compiler": "relay-compiler --src src --schema src/data/schema.graphql",
    "update-schema": "babel-node tools/updateSchema.js",
    "format": "prettier --single-quote --semi=false --write '{src,server,internal}/**/*.js' && eslint --fix src server internal",
    "clean": "rimraf build/client build/server",
  },
  "dependencies": {
    "babel-core": "^6.24.0",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "express": "^4.15.2",
    "express-graphql": "^0.6.11",
    "express-no-favicons": "^0.0.1",
    "farce": "^0.2.5",
    "file-loader": "^1.1.6",
    "found": "^0.3.7",
    "found-relay": "^0.3.0-alpha.1",
    "graphql": "^0.11.7",
    "graphql-relay": "^0.5.4",
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "keycode": "^2.1.9",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "^4.0.0-beta.12",
    "react-relay": "^1.4.1",
    "react-universal-component": "^2.5.1",
    "relay-runtime": "^1.4.1",
    "serialize-javascript": "^1.4.0",
    "todomvc-app-css": "^2.1.0",
    "todomvc-common": "^1.0.4",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-flush-chunks": "^1.1.23"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-relay": "^1.4.1",
    "babel-plugin-universal-import": "^1.2.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-watch": "^2.0.6",
    "commitizen": "^2.9.6",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.8.0",
    "eslint-config-4catalyzer-react": "^0.3.3",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-css-chunks-webpack-plugin": "^2.0.17",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^3.9.1",
    "gulp-command": "^0.1.2",
    "gulp-git": "^1.12.0",
    "gulp-watch": "^4.3.11",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "merge-stream": "^1.0.1",
    "pipe-error-stop": "^0.0.11",
    "poststylus": "^1.0.0",
    "prettier": "^1.5.3",
    "regenerator-runtime": "^0.11.1",
    "relay-compiler": "^1.4.1",
    "rimraf": "^2.6.1",
    "run-sequence": "^1.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "travis-github-status": "^1.4.0",
    "webpack": "^3.5.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-hot-server-middleware": "^0.1.0",
    "write-file-webpack-plugin": "^4.1.0",
    "yargs": "^11.0.0"
  },
  "config": {
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --single-quote --semi=false --write",
        "eslint --fix",
        "git add"
      ]
    }
  }
}
